{
  "name": "photoboothjs",
  "version": "2.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:server": "babel server --out-dir lib && node --inspect ./lib/main.js",
    "start:server:watch": "nodemon -V --watch server --ignore server/config.json --exec npm run start:server",
    "test": "babel server --out-dir lib && node --inspect ./lib/test/test-main.js",
    "test:jest": "jest .spec.js --watch",
    "test:server": "karma start ./server/karma.conf.js",
    "test:camera": "babel server --out-dir lib && node ./lib/test/test-camera.js",
    "test:gpio": "babel server --out-dir lib && node ./lib/test/test-gpio.js",
    "test:serial": "babel server --out-dir lib && node --inspect ./lib/test/test-serialport.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "build": "ng build",
    "build:watch": "ng build --watch",
    "create:config": "cp ./server/config.json.template ./server/config.json",
    "docker:start": "docker-compose -f ./photobooth-compose.yml up",
    "docker:buildImage": "docker build . -t photoboothjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.1.2",
    "@angular/cdk": "8.1.1",
    "@angular/common": "8.1.2",
    "@angular/compiler": "8.1.2",
    "@angular/core": "8.1.2",
    "@angular/forms": "8.1.2",
    "@angular/http": "7.2.15",
    "@angular/material": "8.1.1",
    "@angular/platform-browser": "8.1.2",
    "@angular/platform-browser-dynamic": "8.1.2",
    "@angular/platform-server": "8.1.2",
    "@angular/router": "8.1.2",
    "@babel/polyfill": "7.4.4",
    "@types/body-parser": "^1.17.0",
    "angular-material-icons": "^0.7.1",
    "angular2-qrcode": "^2.0.1",
    "core-js": "^2.6.5",
    "dropbox": "4.0.14",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jasmine": "^3.4.0",
    "jsonfile": "^4.0.0",
    "material-design-icons": "^3.0.1",
    "moment": "^2.24.0",
    "ngx-socket-io": "^2.1.1",
    "ngx-webcam": "^0.2.2",
    "prettier": "^1.16.4",
    "rxjs": "^6.5.2",
    "serialport": "^7.1.5",
    "socket.io": "^2.4.0",
    "socket.io-client": "^2.2.0",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular/compiler-cli": "^8.1.2",
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.4.3",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.14.4",
    "codelyzer": "^4.5.0",
    "eslint-config-google": "^0.9.1",
    "jasmine-core": "^2.99.1",
    "jasmine-spec-reporter": "2.5.0",
    "jest": "^24.7.1",
    "karma": "^4.0.1",
    "karma-browserify": "^6.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-remap-istanbul": "^0.6.0",
    "protractor": "5.4.1",
    "ts-node": "1.2.1",
    "tslint": "^5.15.0",
    "typescript": "3.4.5"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
